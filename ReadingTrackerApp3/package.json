{
  "name": "readingtrackerapp",
  "version": "1.0.0",
  "main": "index.ts",
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation|@expo/vector-icons|expo-.*|react-native-.*|@react-native-.*|@unimodules/.*|unimodules-.*|sentry-expo|native-base|react-native-svg|@react-native-async-storage/async-storage|@react-native-community/async-storage)"
    ]
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/native": "^7.1.11",
    "@reduxjs/toolkit": "^2.8.2",
    "date-fns": "^4.1.0",
    "expo": "~53.0.11",
    "expo-image-picker": "~16.1.4",
    "expo-status-bar": "~2.2.3",
    "formik": "^2.4.6",
    "react": "19.0.0",
    "react-hook-form": "^7.58.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-redux": "^9.2.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-navigation/native-stack": "^7.3.16",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.4.5",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "~0.72.0",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "^19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-react-native": "^18.0.0",
    "prettier": "^3.5.3",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
